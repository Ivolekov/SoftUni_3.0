@using CarDealer.Models;
@model Car

@{
    ViewBag.Title = "Parts";
}

<h1>Detail list of car and parts</h1>
<h2>@Model.Make @Model.Model</h2>
<h3>Distance traveled - @Model.TravelledDistance</h3>

<h2>Parts</h2>
<ul>
    @foreach (var part in Model.Parts)
    {
        <li>@part.Name - $@part.Price</li>
    }
</ul>

