@model IEnumerable<CarDealer.Models.ViewModels.SupplierViewModel>

@{
    ViewBag.Title = "All";
}

<table class="table table-striped">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Number of parts</th>
    </tr>

    @foreach (var supplier in Model)
    {
        <tr>
            <td>
                @supplier.Id
            </td>
            <td>
                @supplier.Name
            </td>
            <td>
               @supplier.NumberOfPartsToSupply
            </td>
        </tr>
    }
</table>





